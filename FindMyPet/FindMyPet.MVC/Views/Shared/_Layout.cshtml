    @*
        @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")
    *@
</head>
<body data-spy="scroll" data-target=".navbar-scrollspy" data-offset="50">

    <!-- Preloader -->
    <div id="preloader">
        <div class="h-100 d-flex justify-content-center align-items-center">
            <div class="ring"></div>
            <div class="ring2"></div>
        </div>
    </div>

    <div id="main-wrapper" class="main-wrapper">
        @Html.Partial("_HeaderNavMenu")

        <!-- Main Content -->
        <div id="main-content">
            <section class="about g-bg-white g-py-20">
                <div class="container">
                    @if (ViewBag.AlertMessage != null)
                    {
                        @Html.Partial("_AlertMessage")
                    }

                    @RenderBody()
                </div>
            </section>
        </div>
        <!-- End Main Content -->
    </div>

    <!-- JS Main Files -->
    <script src="~/CNoteV1.0/assets/vendor/js/jquery.js"></script>
    <script src="~/CNoteV1.0/assets/vendor/js/popper.js"></script>
    <script src="~/CNoteV1.0/assets/vendor/js/bootstrap.js"></script>

    <!-- JS Plugins -->
    <script src="~/CNoteV1.0/assets/vendor/js/owl.carousel.js"></script>
    <script src="~/CNoteV1.0/assets/vendor/js/owl.carousel2.thumbs.js"></script>
    <script src="~/CNoteV1.0/assets/vendor/js/jquery.appear.js"></script>
    <script src="~/CNoteV1.0/assets/vendor/js/jquery.countdown.js"></script>
    <script src="~/CNoteV1.0/assets/vendor/js/countUp.js"></script>
    <script src="~/CNoteV1.0/assets/vendor/js/masonry.pkgd.js"></script>
    <script src="~/CNoteV1.0/assets/vendor/js/imagesloaded.pkgd.js"></script>
    <script src="~/CNoteV1.0/assets/vendor/js/jquery.magnific-popup.js"></script>
    <script src="~/CNoteV1.0/assets/vendor/js/circle-progress.js"></script>
    <script src="~/CNoteV1.0/assets/vendor/js/typed.js"></script>
    <script src="~/CNoteV1.0/assets/vendor/js/scrollreveal.js"></script>

    <!-- jQuery UI Widgets -->
    <script src="~/CNoteV1.0/assets/vendor/js/widget.js"></script>
    <script src="~/CNoteV1.0/assets/vendor/js/mouse.js"></script>
    <script src="~/CNoteV1.0/assets/vendor/js/slider.js"></script>

    <script src="~/CNoteV1.0/cnote-main/assets/js/cnote.js"></script>

    <!-- Thrid Party Plugings -->
    <script src="~/Scripts/bootbox.min.js" ></script>
    @*
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
    *@
    @RenderSection("scripts", required: false)
</body>
</html>
